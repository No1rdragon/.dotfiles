! -- Options -- !
URxvt.internalBorder: 40
URxvt.scrollBar: false
xft.antialias: false

! -- Fonts -- !
URxvt*font: xft:myfont:regular:size=16, xft:Font Awesome 5 Free:size=8
!URxvt*font: envypn7x15
URxvt.letterSpace: 0

! -- Extensions -- !
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

